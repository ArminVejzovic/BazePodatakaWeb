                </tr>
                </thead>
                <tbody>
                <% (elementiIzvjestaja[0])[0].forEach(element => { %>
                    <tr>
                        <td><%= element.kupac_id %></td>
                        <td><%= element.ime_kupca %></td>
                        <td><%= element.prezime_kupca %></td>
                        <td><%= element.broj_narudzbi %></td>
